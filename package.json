{
  "name": "@muleta/faket",
  "version": "1.1.3",
  "description": "Ethiopian Random Name Generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cher-yeros/faket.git"
  },
  "author": "Yerosen Diriba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cher-yeros/faket/issues"
  },
  "homepage": "https://github.com/cher-yeros/faket#readme",
  "main": "dist/index.js",
  "scripts": {
    "test": "nodemon dist/index.js",
